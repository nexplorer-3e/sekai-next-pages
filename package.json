{
  "name": "next-tailwind-ui",
  "version": "1.0.0",
  "description": "Complete Next template for repid development",
  "repository": "git@github.com:rayriffy/next-tailwind-ui.git",
  "author": "Phumrapee Limpianchop <contact@rayriffy.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "node --experimental-wasm-threads --experimental-wasm-bulk-memory --max_old_space_size=10240 ./node_modules/.bin/ts-node --project tsconfig.tools.json scripts/prebuild.ts",
    "build": "next build",
    "dev": "yarn --silent dev:debugger",
    "dev:normal": "next",
    "dev:debugger": "NODE_OPTIONS='--inspect' next dev",
    "start": "next start",
    "cron:hooker": "ts-node --project tsconfig.tools.json scripts/hooker.ts",
    "cron:getDBLatestUpdate": "ts-node --project tsconfig.tools.json scripts/getDBLatestUpdate.ts"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.2.0",
    "@next/bundle-analyzer": "^11.0.1",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "^0.4.1",
    "axios": "^0.21.0",
    "dayjs": "^1.10.6",
    "ical-generator": "^2.2.0",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.1.3",
    "moize": "^6.0.3",
    "next": "^11.0.1",
    "next-compose-plugins": "^2.2.1",
    "next-offline": "^5.0.5",
    "postcss": "^8.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sitemap": "^7.0.0",
    "storeon": "^3.1.1",
    "tailwindcss": "^2.2.4",
    "tailwindcss-filters": "^3.0.0",
    "web-api-hooks": "^3.0.2",
    "webpack": "^5.45.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/axios": "^0.14.0",
    "@types/fs-extra": "^9.0.12",
    "@types/lodash": "^4.14.171",
    "@types/markdown-to-jsx": "^7.0.1",
    "@types/node": "^16.3.2",
    "@types/react": "^17.0.14",
    "@types/wicg-mediasession": "^1.1.2",
    "babel-plugin-lodash": "^3.3.4",
    "cwait": "^1.1.2",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
